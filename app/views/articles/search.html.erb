<div class="contents">
  <%= form_tag('/articles/search', method: :get) do %>
    <div class="search_form">
      <input class="search_article" name="keyword" placeholder="記事名で検索" type="text" value="">
        <button class="btn_search" title="検索" type="submit">検索</button>
      </input>
    </div>
  <% end %>
  <div class="searched_articles">
    <%= render partial: "article", collection: @articles %>
  </div>
</div>