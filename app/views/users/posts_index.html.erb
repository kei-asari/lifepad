<div class="contents">
  <div class="users_top clearfix">
    <div class="users_icon">
      <%= current_user.username %>さん
    </div>
    <div class="users_btn posts_show">
      <div class="btn">
        <%= link_to "Clips", user_path(current_user), :method => :get %>
      </div>
    </div>
    <div class="users_btn log_out">
      <div class="btn">
        <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
      </div>
    </div>
  </div>
  <div class="users_bottom">
    <%= render partial: "article", collection: @articles %>
  </div>
</div>